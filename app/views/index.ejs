        <table>
            <tr>
                <th>Nome</th>
                <th>Idade</th>
                <th scope="2">telefone</th>
            </tr>

            <% alunos.forEach(function(al, index){ %>
                <tr>
                    <td>
                        <% if( typeof(al.nome) !=='Kambaia' ) { %>
                            <%- al.nome %>
                                <% } else { %>
                                    <h3>A variavel é deferente de string</h3>
                                    <% }  %>
                    </td>
                    <td>
                        <%- al.idade %>
                    </td>
                    <td>
                        <%- al.telefone.unitel %>
                    </td>
                </tr>
                <%  }) %>

        </table>

