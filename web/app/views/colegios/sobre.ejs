
 
    <% include partials/head %>
	<div class="container">

			<div class="card-body">
				<h2 class="card-title">Meu Perfil Completo</h2>
			<hr>
			<% for(var i=0; i<todosDadosC.length; i++) { %>
					<div>
						<h1>convocatoria Enviadas</h1>
						<% if(todosDadosC.length > 0) { %>
											
							<span>Nome do Administrador: <%=nome%></span><br>
							<span>Email do Administrador: <%=email %></span><br>
							<span>Complexo Escolar: <%=todosDadosC[0].Nome_Colegio%></span><br>
							<span>Data de fundação do colégio: <%=todosDadosC[0].data_fundacao%></span><br>
							<span>província na qual se encontra o colegio: <%=todosDadosC[0].endereco.provincia%></span><br>
							<span>Municipio de : <%=todosDadosC[0].endereco.municipio%></span><br>
							<span>Bairro: <%=todosDadosC[0].endereco.bairro%></span><br>
							<span>Rua: <%=todosDadosC[0].endereco.rua%></span><br>
				
							<span>Telefone do colégio: <%=todosDadosC[0].contacto.contacto_colegio%></span><br>
							<span>Telefone 1º op: <%=todosDadosC[0].contacto.contacto_movicel%></span><br>
							<span>Telefone 2º op<%=todosDadosC[0].contacto.contacto_unitel%></span><br>
						<% } %>
					
					</div>
				<% } %>
				
				<button type="submit" class="btn btn-primary" id="btn_terminar">Salvar</button>
			</div>
		</div>
							
	